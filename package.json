{
  "name": "rubberbands",
  "version": "1.0.1",
  "description": "elastic search transaction wrapper",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape ./test/*.test.js | tap-spec",
    "jshint": "jshint -c ./.jshintrc --exclude-path .gitignore .",
    "coverage": "istanbul cover tape ./test/*.test.js && istanbul check-coverage --statements 95 --functions 95 --lines 95 --branches 95",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foundersandcoders/rubberbands.git"
  },
  "keywords": [
    "elastic",
    "search",
    "esta",
    "database",
    "wrapper",
    "orm"
  ],
  "author": "William Fisher, Besart Shyti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foundersandcoders/rubberbands/issues"
  },
  "pre-commit": [
    "jshint",
    "coverage"
  ],
  "homepage": "https://github.com/foundersandcoders/rubberbands",
  "devDependencies": {
    "istanbul": "^0.3.13",
    "jshint": "^2.7.0",
    "pre-commit": "^1.0.6",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0"
  },
  "dependencies": {
    "esta": "^4.0.0"
  }
}
